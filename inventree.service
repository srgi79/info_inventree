[Unit]
Description=Inventree Server Service
Wants=network.target
After=multi-user.target

[Service]
User=inventree
ExecStartPre=/bin/sleep 10
ExecStart=/bin/bash -c 'source home/inventree/env/bin/activate && cd /home/inventree/src && invoke server -a 192.168.31.100:8000' &
Restart=always

[Install]
WantedBy=multi-user.target